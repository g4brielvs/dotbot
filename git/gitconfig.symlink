[alias]
        co = checkout
        exclude = !sh -c 'echo "$1" >> .git/info/exclude' -
  
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true

[core]
        excludesfile = ~/.gitignore

[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
	
